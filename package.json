{
  "devDependencies": {
    "csv-parse": "^4.3.1",
    "dotenv": "^6.2.0",
    "jest": "^24.1.0",
    "sequelize-cli": "^5.4.0",
    "serverless": "^1.36.3",
    "serverless-jest-plugin": "^0.2.1",
    "serverless-offline": "^4.2.1"
  },
  "scripts": {
    "sls-test": "sls invoke test",
